{
  "name": "@robertotauille/react-components",
  "author": "Roberto Tauille",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-components.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "build-tailwind": "npx tailwindcss -o ./dist/tailwind.css --minify",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=17",
    "@headlessui/react": "^1.7.5",
    "@heroicons/react": "^2.0.13",
    "date-fns": "^2.29.3",
    "htmr": "^1.0.2",
    "react-day-picker": "^8.3.7",
    "react-input-mask": "^3.0.0-alpha.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/tw-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tw-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.5",
    "@heroicons/react": "^2.0.13",
    "date-fns": "^2.29.3",
    "htmr": "^1.0.2",
    "react-day-picker": "^8.3.7",
    "react-input-mask": "^3.0.0-alpha.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-input-mask": "^3.0.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tailwindcss": "^3.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
